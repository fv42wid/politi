%h1 Candidates

= render :partial => 'layouts/flash_display'

= form_tag candidates_path, :method => 'get' do
  %p
    = text_field_tag :search, params[:search]
    = submit_tag "Search", :name => nil

%div#candidates
  = render :partial => 'candidates'

%br

= link_to 'New Candidate', new_candidate_path
%p= will_paginate @candidates
